<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	android:id="@+id/NewEvent" android:layout_width="fill_parent" android:layout_height="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<ImageButton
		android:id="@+id/BiddingBtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="15dip"
		android:layout_alignParentTop="true"
		android:background="#a0000000"
		android:layout_centerHorizontal="true"
		android:src="@drawable/biddingbtn">
	</ImageButton>
	
	<ImageButton
		android:id="@+id/SalesBtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/BiddingBtn"
		android:background="#a0000000"
		android:layout_alignLeft="@+id/BiddingBtn"
		android:layout_marginTop="10dip"
		android:src="@drawable/salesbtn"
	>
	</ImageButton>
	
	<ImageButton
		android:id="@+id/DrinkBtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="#a0000000"
		android:layout_below="@+id/SalesBtn"
		android:layout_alignLeft="@+id/SalesBtn"
		android:layout_marginTop="10dip"
		android:src="@drawable/bottlebtn"
	>
	</ImageButton>
	<ImageButton
		android:id="@+id/DressBtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/DrinkBtn"
		android:layout_alignLeft="@+id/DrinkBtn"
		android:background="#a0000000"
		android:layout_marginTop="10dip"
		android:src="@drawable/cosbtn"
	>
	</ImageButton>
	<ImageButton
		android:id="@+id/EtcBtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/DressBtn"
		android:layout_alignLeft="@+id/DressBtn"
		android:layout_marginTop="10dip"
		android:background="#a0000000"
		android:src="@drawable/etcbtn"
	>
	</ImageButton>


</RelativeLayout>
